{
  "expo": {
    "name": "Bayanihan",
    "slug": "bayanihan-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ab_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF9F0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.firebase.bayanihan" 
    },
    "web": {
      "favicon": "./assets/images/app_logo.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Bayanihan to use your location."
        }
      ],
      "expo-build-properties",
      "expo-video",
      "expo-image-picker"
    ],
    "jsEngine": "hermes",
    "extra": {
      "eas": {
        "projectId": "b2922363-ee56-45ca-b956-cc0ba90e8b25"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {},
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
    "translucent": true,
    "backgroundColor": "#000000"
  },
    "owner": "louihv"
  }
}
