{
  "expo": {
    "name": "Bayanihan-Mobile",
    "slug": "Bayanihan-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ab_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ab_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF7EC",
      "permissions": [
        "location"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to display your position on the map and provide personalized recommendations."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ab_logo.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "android.permission.ACCESS_FINE_LOCATION",
          "android.permission.ACCESS_COARSE_LOCATION"
        ],
        "config": {
          "googleMaps": {
            "apiKey": "AIzaSyBDtlY28p-MvLHRtxnjiibSAadSETvM3VU"
          }
        }
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.louihv.BayanihanMobile"
    },
    "web": {
      "favicon": "./assets/images/ab_logo.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow this app to use your location to show your position on the map."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload donation images."
        }
      ]
    ],
    "jsEngine": "jsc",
    "extra": {
      "eas": {
        "projectId": "ad4bfadc-bdef-4cb4-9d6e-d1bff4a618a6"
      }
    }
  }
}
